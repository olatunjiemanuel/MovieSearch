    ]
  },
  "dependencies": {
    "@eslint/create-config": "^0.4.6",
    "eslint": "^8.56.0",
    "expo": "~50.0.2",
    "expo-status-bar": "~1.11.1",
    "husky": "^9.0.11",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prompt-sync": "^4.2.0",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/react-native": "^12.4.3",
