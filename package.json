    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
